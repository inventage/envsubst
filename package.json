    "test": "ava"
  },
  "dependencies": {
    "cli-table3": "^0.6.3",
    "escape-string-regexp": "^4.0.0",
    "globby": "^11.1.0",
    "meow": "^9.0.0",
    "string.prototype.matchall": "^4.0.8"
  },
  "devDependencies": {
    "@types/node": "^20.4.9",
    "ava": "^5.3.1",
    "dotenv": "^16.3.1",
    "execa": "^5.1.1",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1"
  },
  "keywords": [
    "env",
    "environment",
    "envsubst",
    "node",
    "npm",
    "variables",
    "vars"
